<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Prevencao">

    <View
        android:id="@+id/containerComponentes5"
        style="@style/ContainerComponents"
        android:layout_width="352dp"
        android:layout_height="627dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="12dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="144dp"
        android:layout_height="37dp"
        android:text="Prevenção"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/containerComponentes5"
        app:layout_constraintStart_toStartOf="@+id/containerComponentes5"
        app:layout_constraintTop_toBottomOf="@+id/prevencao2" />

    <ImageView
        android:id="@+id/prevencao2"
        android:layout_width="136dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerComponentes5"
        app:layout_constraintEnd_toEndOf="@+id/containerComponentes5"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/containerComponentes5"
        app:layout_constraintTop_toTopOf="@+id/containerComponentes5"
        app:layout_constraintVertical_bias="0.013"
        app:srcCompat="@drawable/chuva" />
</androidx.constraintlayout.widget.ConstraintLayout>